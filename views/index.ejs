      <%a.push(meetings[i].id)%>
 <td><%=meetings[i].duration%> minutes</td>
     <td><a href=" <%=meetings[i].join_url%>">Join</a></td>
       <td><%=new Date(meetings[i].start_time.split("T")[0]).toDateString()%></td>
       <td><%=new Date(meetings[i].created_at).toDateString()%></td>
      </tr>
  

 
    <%}%>
</table>
